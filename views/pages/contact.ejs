<%- include('../partials/head') -%>
<%- include('../partials/nav') -%>
    <div class="content">
        <h1>Contact us</h1>
        <p>Any queries or concerns? Feel free to send us feedback.</p>
        <form action="/formsubmitted" method="POST">
            <div class="form-group">
                <label>Name: </label><input type="text" id="visitorName">
            </div>
            <div class="form-group">
                <label>Email address: </label><input type="text" id="visitorEmail">
            </div>
            <div class="form-group">
                <label>Comment: </label><input type="textarea" id="visitorComment">
            </div>

            <label></label><input type="button" onclick="contactSubmit()" value="Submit">
        </form>
    </div>
    <%- include('../partials/footer') -%>